sudo ./gatee

while read -r line
do    
    if [[ -d $line ]]; then
        cd "$line"
        echo "Changed directory to $line";
        break;
    elif [[ -f $line ]]; then
        nano "$line" < /dev/tty ;
        echo "Opened $line with nano";
        break;
    else
        echo "ERROR.";
        break;
    fi
done < "/.gatee_cd"
